<structure name="manifest">
    <field alias="short_name" minOccurs="0" name="shortName" type="java.lang.String"/>
    <field minOccurs="0" name="name" type="java.lang.String"/>
    <structure comment="must include a 192px and a 512px sized icons " maxOccurs="unbounded" minOccurs="0" name="icons">
        <field name="src" type="java.lang.String"/>
        <field minOccurs="0" name="type" type="java.lang.String"/>
        <field name="sizes" type="java.lang.String"/>
    </structure>
    <field alias="start_url" minOccurs="0" name="startUrl" type="java.lang.String"/>
    <field comment="fullscreen, standalone, or minimal-ui" minOccurs="0" name="display" type="java.lang.String"/>
    <field minOccurs="0" name="scope" type="java.lang.String"/>
    <field alias="background_color" minOccurs="0" name="backgroundColor" type="java.lang.String"/>
    <field alias="theme_color" minOccurs="0" name="themeColor" type="java.lang.String"/>
    <field alias="prefer_related_applications" minOccurs="0" name="preferRelatedApplications" type="java.lang.Boolean"/>
    <structure alias="related_applications" maxOccurs="unbounded" minOccurs="0" name="relatedApplications">
        <field name="platform" type="java.lang.String"/>
        <field name="id" type="java.lang.String"/>
    </structure>
</structure>
